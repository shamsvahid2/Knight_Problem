	c4 [label=c4 fillcolor=white style=filled]
	b6 -> c4 [label=2 color=red]
	a5 [label=a5 fillcolor=white style=filled]
	c4 -> a5 [label=3 color=red]
	a4 [label=a4 fillcolor=white style=filled]
	c5 [label=c5 fillcolor=white style=filled]
	a4 -> c5 [label=1 color=green]
	b3 [label=b3 fillcolor=white style=filled]
	c5 -> b3 [label=2 color=green]
	a5 [label=a5 fillcolor=white style=filled]
	b3 -> a5 [label=3 color=green]
	a4 [label=a4 fillcolor=white style=filled]
	c5 [label=c5 fillcolor=white style=filled]
	a4 -> c5 [label=1 color=blue]
	b7 [label=b7 fillcolor=white style=filled]
	c5 -> b7 [label=2 color=blue]
	a5 [label=a5 fillcolor=white style=filled]
	b7 -> a5 [label=3 color=blue]
	a4 [label=a4 fillcolor=white style=filled]
	b2 [label=b2 fillcolor=white style=filled]
	a4 -> b2 [label=1 color=orange]
	c4 [label=c4 fillcolor=white style=filled]
	b2 -> c4 [label=2 color=orange]
	a5 [label=a5 fillcolor=white style=filled]
	c4 -> a5 [label=3 color=orange]
}
